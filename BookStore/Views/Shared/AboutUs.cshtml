
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>AboutUs</title>
</head>
<body>
    <h1>Hello From About us page of application</h1>
    @DateTime.Now.ToString()

    @{ int a = 10;}
    @switch (a)
    {
        case 1:<h1>I am 1</h1>
            break;
        case 2:<h1>I am 2</h1>
            break;
        case 3:<h1>I am 3</h1>
            break;
        case 4:<h1>I am 4</h1>
            break;
        default:<h1>I am Default</h1>
            break;

    }
    @for (int i = 0; i < 5; i++)
    {
        <h1>@i</h1>
    }
    @{ var list = new List<int>() { 1, 2, 3, 4, 5 };}
    @foreach(var item in list)
    {
        <h2>@item</h2>
    }
</body>
</html>
